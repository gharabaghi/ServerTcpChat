<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  
  <connectionStrings>
    <add name="ServerTcpChat.Properties.Settings.Server_DBConnectionString"
      connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\Server_DB.mdf;Integrated Security=True"
      providerName="System.Data.SqlClient" />
    <add name="ServerTcpChat.Properties.Settings.TcpServer_DBConnectionString"    
      connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\DataBase\TcpServer_DB.mdf;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>

  <appSettings>
    <add key="ServerIP" value="127.0.0.1" />
    <add key="ServerUdpPort" value="9090"/>
    <add key="UdpCheckData" value="9999"/>
  </appSettings>
  
  
</configuration>